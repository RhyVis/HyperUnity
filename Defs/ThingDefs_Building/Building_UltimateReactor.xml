<?xml version="1.0" encoding="UTF-8"?>
<Defs>

  <DesignatorDropdownGroupDef>
    <defName>R_UReactor_Resource</defName>
  </DesignatorDropdownGroupDef>
  <DesignatorDropdownGroupDef>
    <defName>R_UReactor_Meal</defName>
  </DesignatorDropdownGroupDef>
  <DesignatorDropdownGroupDef>
    <defName>R_UReactor_Universal</defName>
  </DesignatorDropdownGroupDef>

  <ThingDef Name="RNormalUReactorBase" ParentName="BuildingBase" Abstract="True">
    <description>这个容器内可以消耗电力来无限生成特定物品。</description>
    <thingClass>HyperUnity.Building_WithoutZoneSettings</thingClass>
    <graphicData>
      <texPath>Things/Building/Furniture/ShelfSmall</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(2,2)</drawSize>
      <damageData>
        <enabled>False</enabled>
      </damageData>
    </graphicData>
    <altitudeLayer>Building</altitudeLayer>
    <size>(1, 1)</size>
    <building>
      <preventDeteriorationOnTop>True</preventDeteriorationOnTop>
      <ignoreStoredThingsBeauty>True</ignoreStoredThingsBeauty>
    </building>
    <costList>
      <Steel>30</Steel>
    </costList>
    <tickerType>Normal</tickerType>
    <passability>PassThroughOnly</passability>
    <castEdgeShadows>False</castEdgeShadows>
    <canOverlapZones>False</canOverlapZones>
    <fillPercent>0.1</fillPercent>
    <statBases>
      <MaxHitPoints>250</MaxHitPoints>
      <Mass>8</Mass>
      <WorkToBuild>100</WorkToBuild>
    </statBases>
    <designationCategory>R_MainCategory</designationCategory>
    <thingCategories>
      <li>BuildingsMisc</li>
    </thingCategories>
    <minifiedDef>MinifiedThing</minifiedDef>
    <inspectorTabs>
      <li>ITab_Storage</li>
    </inspectorTabs>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <shortCircuitInRain>False</shortCircuitInRain>
        <basePowerConsumption>500</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable"/>
      <li Class="CompProperties_Forbiddable"/>
      <li Class="HyperUnity.CompProperties_PreventDeterioratingOrRotting"/>
    </comps>
  </ThingDef>

  <ThingDef Name="RResourceUReactorBase" ParentName="RNormalUReactorBase" Abstract="True">
    <designatorDropdown>R_UReactor_Resource</designatorDropdown>
  </ThingDef>
  <ThingDef Name="RMealUReactorBase" ParentName="RNormalUReactorBase" Abstract="True">
    <designatorDropdown>R_UReactor_Meal</designatorDropdown>
  </ThingDef>
  <ThingDef Name="RUniversalUReactorBase" ParentName="RNormalUReactorBase" Abstract="True">
    <description>这个容器内可以消耗电力来无限生成特定物品，可以切换几种许可类型。</description>
    <designatorDropdown>R_UReactor_Universal</designatorDropdown>
  </ThingDef>

  <!-- UReactor Resource -->

  <ThingDef ParentName="RResourceUReactorBase">
    <defName>R_UReactor_Medicine</defName>
    <label>超越反应器（超联结医药）</label>
    <building>
      <fixedStorageSettings>
        <priority>Critical</priority>
        <filter>
          <thingDefs>
            <li>R_SpecialMedicine</li>
          </thingDefs>
        </filter>
      </fixedStorageSettings>
      <defaultStorageSettings>
        <priority>Critical</priority>
        <filter>
          <thingDefs>
            <li>R_SpecialMedicine</li>
          </thingDefs>
        </filter>
      </defaultStorageSettings>
    </building>
    <comps>
      <li Class="HyperUnity.CompProperties_UltimateReactor">
        <checkInterval>3600</checkInterval>
        <minCount>5</minCount>
        <refillCount>20</refillCount>
        <eachItemCost>100</eachItemCost>
        <itemDefName>R_SpecialMedicine</itemDefName>
      </li>
    </comps>
  </ThingDef>

